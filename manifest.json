{
  "name": "Redux on Programmes",
  "version": "0.1",
  "description": "Embeds Redux videos on /programmes episode pages",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://www.bbc.co.uk/programmes/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": [
    "https://i.bbcredux.com/",
    "http://www.bbc.co.uk/programmes/"
  ]
}
